const numbers = [1, 2, 6, 9, 4, 7, 0]



function max_num(numbers: number[]) {
    let max = numbers[0]


    for (let index = 1; index < numbers.length; index++) {
        if (numbers[index] > max) {
            max = numbers[index]
        }
    }
    return max

}

const maxnumbers = max_num(numbers)
console.log(maxnumbers)

